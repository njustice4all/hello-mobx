{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "extends": ["plugin:react/recommended"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-console": 0,
    "global-require": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0
  }
}
